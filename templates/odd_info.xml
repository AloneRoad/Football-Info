<?xml version="1.0" encoding="UTF-8"?>
<WorlcupOdds version="$version" last_update="$last_update">
    <odd_info source="$info.source">
        <info>$info.match</info>
        <full_time>
            $info.team_1: +$info.ah  @$info.ah_team_1
            $info.team_2: -$info.ah  @$info.ah_team_2
                          +$info.ou  @$info.ou_team_1
                          -$info.ou  @$info.ou_team_2
        </full_time>
        <first_half>
            $info.team_1: +$info.ah1st  @$info.ah1st_team_1
            $info.team_2: -$info.ah1st  @$info.ah1st_team_2
                          +$info.ou1st  @$info.ou1st_team_1
                          -$info.ou1st  @$info.ou1st_team_2
        </first_half>
    </odd_info>
</WorlcupOdds>
